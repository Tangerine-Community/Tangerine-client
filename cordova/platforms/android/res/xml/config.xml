<?xml version='1.0' encoding='utf-8'?>
<widget id="org.rti.tangerine" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="CBLite">
        <param name="android-package" value="com.couchbase.cblite.phonegap.CBLite" />
        <param name="onload" value="true" />
    </feature>
    <name>Tangerine</name>
    <description>
        Mobile data collection system.
    </description>
    <author email="ict-admin@rti.org" href="github.com/Tangerine-Community/Tangerine">
        Tangerine dream team
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
